/* pages/booking/booking.wxss */
.tab-header {
    height: 162rpx;
    /* 由于小程序限制，暂时只能用base64形式，后面需换成线上图片地址 */
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: cover;
    overflow: hidden;
}

.tab-header-inner {
    display: flex;
    width:380rpx;
    height:70rpx;
    margin: 28rpx 20rpx;
    background:rgba(96,58,170,1);
    border-radius:35rpx;
}

.tab-header-inner .tab {
    width: 180rpx;
    height: 100%;
    padding-left: 20rpx;
    line-height: 70rpx;
    font-size: 34rpx;
    color: #FEFEFF;
}

.tab-header-inner .tab.active {
    width:200rpx;
    padding-left: 0;
    text-align: center;
    background:rgba(142,67,246,1);
    border-radius:35rpx;
}

.notify {
    display: flex;
    align-items: center;
    width: 95%;
    height:130rpx;
    margin: 0 auto;
    margin-top: -14rpx;
    padding: 0 18rpx;
    background:rgba(255,255,255,1);
    border:1rpx solid rgba(242, 242, 242, 1);
    border-radius:22rpx;
}

.notify .mailbox {
    width: 113rpx;
    height: 137rpx;
    margin-top: -12rpx;
}

.notify .refresh {
    width: 84rpx;
    height: 84rpx;
}

.notify text {
    margin: -10rpx 60rpx 0 42rpx;
    color: #666;
    font-size: 24rpx;
}

.cards-box {
    margin-top: 26rpx;
    padding: 0 38rpx;
    padding-bottom: 189rpx;
    background:rgba(255,255,255,1);
    border:1rpx solid rgba(242, 242, 242, 1);
    border-radius:50rpx 50rpx 0 0;
}

.cards-box .card {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 40rpx 0;
    border-bottom: 1rpx solid #D8DCEB;
}

.card-left {
    display: flex;
}

.card .room-image {
    width:172rpx;
    height:124rpx;
    margin-right: 20rpx;
    border-radius:6rpx;
}

.room-info {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    font-size: 24rpx;
}

.room-info .name {
    color: #3F4045;
    font-size: 34rpx;
    font-weight: 500;
    line-height: 1;
}

.room-info .name.free .tag,
.room-info .name.meeting .tag {
    display: inline-block;
    height:26rpx;
    line-height: 26rpx;
    text-align: center;
    vertical-align: middle;
    margin-top: -5rpx;
    margin-left: 5rpx;
    font-size: 22rpx;
    color: #fff;
    border-radius:4rpx;
}

.room-info .name.free .tag{
    width: 60rpx;
    background:rgba(69,204,152,1);
}

.room-info .name.meeting .tag {
    width: 80rpx;
    background:rgba(253,105,32,1);
}

.room-info .num {
    color: #999;
}

.room-info .text {
    margin-right: 14rpx;
    color: #666;
}

.free-time {
    color: #A1B7FF;
}

.free-time text:not(.text) {
    display: inline-block;
    width:32rpx;
    height:34rpx;
    line-height: 34rpx;
    text-align: center;
    font-size: 22rpx;
    color: #fff;
    background:rgba(138,166,255,1);
    border-radius:6rpx;
}

.access {
    display: flex;
    align-items: center;
    width:170rpx;
    height:52rpx;
    padding-left: 16rpx;
    font-size: 24rpx;
    color: #8A46E1;
    background:rgba(198,205,230,0);
    border:1px solid rgba(168, 114, 237, 1);
    border-radius:26rpx;
}

.access image {
    width: 16rpx;
    height: 16rpx;
    margin-left: 4rpx;
}
