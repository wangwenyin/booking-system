/* pages/booking-info/booking-info.wxss */
.booking-info-box .my-calendar {
    background-image:url(data:image/png;base64,);
    background-size: cover;
    background-repeat: no-repeat;
}

.calendar .weeks {
    opacity: 1;
}

.calendar .fs28  {
    font-size: 30rpx;
}

.indicates {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 68rpx;
    padding: 0 20rpx;
    font-size: 22rpx;
    background: #F8F8F8;
}

.indicate-item text:first-child {
    display: inline-block;
    width:22rpx;
    height:22rpx;
    margin-right: 8rpx;
    border-radius:4rpx;
    vertical-align: middle;
}

.indicates .indicate-item:nth-child(1) text:first-child {
    background:rgba(101,137,252,1);
}
.indicates .indicate-item:nth-child(2) text:first-child {
    background:rgba(69,204,152,1);
}
.indicates .indicate-item:nth-child(3) text:first-child {
    background:rgba(222,223,232,1);
}
.indicates .indicate-item:nth-child(4) text:first-child {
    background:rgba(255,99,22,1);
}
.indicates .indicate-item:nth-child(5) text:first-child {
    background:rgba(255,163,17,1);
}

.time-blocks {
    display: flex;
    flex-wrap: wrap;
    padding: 36rpx 20rpx;
    background: #fff;
}

.time-blocks .block {
    position: relative;
    display: flex;
    width:114rpx;
    height:72rpx;
    line-height: 72rpx;
    text-align: center;
    margin-right: 5rpx;
    margin-bottom: 6rpx;
    color: #fff;
    border-radius:6rpx;
}

.block view {
    height: 100%;
}

.block .time {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background: transparent;
    color: #fff;
}

.time-blocks .block .free {
    background: #45CC98;
}

.time-blocks .block .no-meeting {
    background: #FFA311;
}

.time-blocks .block .held-meeting {
    background: #FF6316;
}

.time-blocks .block .disabled {
    background: #DEDFE8;
    color: #A9ABBB;
}

.free-time {
    margin: 29rpx 0 19rpx 0;
    font-size: 26rpx;
    font-weight: 500;
    color: #757EE9;
    text-align: center;
}

.to-booking-btn {
    width:90%;
    height:84rpx;
    line-height: 84rpx;
    text-align: center;
    margin: 0 auto;
    font-size: 32rpx;
    color: #FEFEFF;
    background:linear-gradient(90deg,rgba(114,135,241,1),rgba(133,81,192,1));
    border-radius:42rpx;
}